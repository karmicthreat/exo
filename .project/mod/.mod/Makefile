#----------------------------------------
#   __   __        _      _    _        
#   \ \ / /_ _ _ _(_)__ _| |__| |___ ___
#    \ V / _` | '_| / _` | '_ \ / -_|_-<
#     \_/\__,_|_| |_\__,_|_.__/_\___/__/
#                                       
include ../../vars.mk

#----------------------------------------
#    ___      _        
#   | _ \_  _| |___ ___
#   |   / || | / -_|_-<
#   |_|_\\_,_|_\___/__/
#                      
obj/$(TARGET):
	mkdir -p $@

.PHONY: %.o
%.o: obj/$(TARGET)/%.o
	@echo "Built $@"

obj/$(TARGET)/%.o: %.c obj/$(TARGET)
	$(CC) $(FLAGS) $(INC) -c $< -o $@

.PHONY: clean
clean:
	rm -rf obj
